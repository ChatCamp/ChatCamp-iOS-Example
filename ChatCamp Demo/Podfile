# Uncomment the next line to define a global platform for your project
platform :ios, '11.0'

target 'ChatCamp Demo' do
  # Comment the next line if you're not using Swift and don't want to use dynamic frameworks
  use_frameworks!
    
    pod 'ChatCampUIKit', :git => 'https://github.com/ChatCamp/ChatCamp-iOS-UI-Kit', :branch => 'staging'
    pod 'Fabric'
    pod 'Crashlytics'
    
end
